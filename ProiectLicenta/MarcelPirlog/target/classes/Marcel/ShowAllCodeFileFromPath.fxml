<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Marcel.controllers.uicontrollers.ShowAllCodeFileFromPathController">
    <AnchorPane prefHeight="700.0" prefWidth="1000.0" style="-fx-background-color: #231B12;">
        <TableView fx:id="listWithAllFiles" layoutX="10.0" layoutY="180.0" prefHeight="500.0" prefWidth="980.0" stylesheets="@style.css">
            <columns>
                <TableColumn fx:id="fileName" prefWidth="365.0" text="Name" />
                <TableColumn fx:id="fileCreateDate" minWidth="0.0" prefWidth="187.0" text="Create Date" />
                <TableColumn fx:id="fileLastUpdate" minWidth="0.0" prefWidth="241.0" text="Last Update" />
                <TableColumn fx:id="fileSize" prefWidth="186.0" text="Size" />
            </columns>
        </TableView>
      <Button fx:id="stopThread" layoutX="851.0" layoutY="48.0" mnemonicParsing="false" onAction="#endThreadAction" prefHeight="40.0" prefWidth="200.0" style="-fx-background-color: #F56C57;" text="Finish" AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="70.0">
         <font>
            <Font name="System Bold" size="25.0" />
         </font></Button>
      <Button fx:id="backToProfileButton" alignment="CENTER" layoutX="57.0" layoutY="48.0" mnemonicParsing="false" onAction="#backToProfileButtonFct" prefWidth="200.0" style="-fx-background-color: #F56C57;" stylesheets="@style.css" text="Back" textAlignment="CENTER" AnchorPane.leftAnchor="100.0" AnchorPane.topAnchor="70.0">
         <font>
            <Font name="System Bold" size="25.0" />
         </font></Button>
    </AnchorPane>
</AnchorPane>
