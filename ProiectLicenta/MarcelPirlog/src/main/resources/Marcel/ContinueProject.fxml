<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Marcel.controllers.uicontrollers.ContinueProjectController">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #231B12;">
         <children>
            <Button fx:id="backButton" layoutX="50.0" layoutY="42.0" mnemonicParsing="false" onAction="#back" prefHeight="44.0" prefWidth="120.0" style="-fx-background-color: #F56C57; -fx-text-fill: #000000;" text="Back" AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="42.0">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <ListView fx:id="projList" layoutX="30.0" layoutY="108.0" prefHeight="200.0" prefWidth="560.0" style="-fx-background-color: #F77604;" stylesheets="@style.css" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0" />
            <Button fx:id="cotinueBtn" layoutX="438.0" layoutY="42.0" mnemonicParsing="false" onAction="#continueProject" prefHeight="44.0" prefWidth="120.0" style="-fx-background-color: #F56C57; -fx-text-fill: #000000;" text="Continue" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="42.0">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <TextField fx:id="newLocation" layoutX="30.0" layoutY="328.0" prefHeight="40.0" prefWidth="300.0" promptText="Project folder location" style="-fx-background-color: #B8D20B; -fx-prompt-text-fill: #000000;" />
            <Label fx:id="errorLabelLocation" alignment="CENTER" layoutX="370.0" layoutY="328.0" prefHeight="40.0" prefWidth="200.0" text="Invalid location" textFill="RED">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
